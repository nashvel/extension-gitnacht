{
  "name": "git-nacht",
  "displayName": "Git Nacht",
  "description": "One-click git add . in your project root. Simple, fast, and sassy.",
  "version": "0.0.2",
  "icon": "assets/git-nacht.png",
  "publisher": "nacht",
  "repository": {
    "type": "git",
    "url": "https://github.com/nashvel/extension-gitnacht.git"
  },
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "git-nacht.addAll",
        "title": "Git Nacht: Add All"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/node": "^14.14.31",
    "@types/vscode": "^1.60.0",
    "@typescript-eslint/eslint-plugin": "^4.16.0",
    "@typescript-eslint/parser": "^4.16.0",
    "eslint": "^7.21.0",
    "glob": "^7.1.6",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  },
  "dependencies": {
    "lucide-react": "^0.554.0"
  }
}
